<h1>New Lease</h1>
<form method="POST" action="/users/{{userId}}/leases">
    <div>
        <label for="homeImage">image URL</label>
        <input type="text" name="homeImage">
    </div>
    <div>
        <label for="address">address</label>
        <input type="text" name="address">
    </div>
    <div>
        <label for="tenants">tenants</label>
        <input type="text" name="tenants">
    </div>
    <div>
        <label for="rentDue">rent due</label>
        <input type="text" name="rentDue">
    </div>
    <div>
        <label for="rentPaid">rent paid</label>
        <input type="text" name="rentPaid">
    </div>
    <button type="submit">Add Lease</button>

</form>