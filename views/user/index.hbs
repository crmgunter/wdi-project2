<h1>Users</h1>

{{#each users}}
<div>    
    <h2>{{username}}</h2>
    <h4>{{firstName}} {{lastName}}</h4>
    <p>{{email}}</p>
    <p>{{phoneNumber}}</p>
    <img src="{{image}}"/>
    <h4>Properties</h4>
    {{#each properties}}
    <p>{{address}}</p>
    <p>{{landlord}}</p>
    <p>{{price}}</p>
    <p>{{description}}</p>
    {{/each}}
</div>

{{/each}}